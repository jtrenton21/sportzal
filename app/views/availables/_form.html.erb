<%= simple_form_for(@available) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :weekday %>
    <%= f.input :start_at %>
    <%= f.input :end_at %>
    <%= f.input :timeframe %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
